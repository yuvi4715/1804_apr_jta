<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
  	<param-name>BestQBAllTime</param-name>
  	<param-value>Joe Montana</param-value>
  </context-param>
  <servlet>
  	<servlet-name>HelloWorld</servlet-name>
  	<display-name>HelloWorld</display-name>
  	<description></description>
  	<servlet-class>com.revature.servlets.HelloWorld</servlet-class>
  	<load-on-startup>3</load-on-startup>
  </servlet>
  <servlet>
  	<servlet-name>SecondServlet</servlet-name>
  	<display-name>SecondServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.servlets.SecondServlet</servlet-class>
  	<load-on-startup>2</load-on-startup>
  </servlet>
  <servlet>
  	<servlet-name>ThirdServlet</servlet-name>
  	<display-name>ThirdServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.servlets.ThirdServlet</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
  	<servlet-name>ServletContextConfig</servlet-name>
  	<display-name>ServletContextConfig</display-name>
  	<description></description>
  	<servlet-class>com.revature.servlets.ServletContextConfig</servlet-class>
  	<init-param>
  		<param-name>BestQB</param-name>
  		<param-value>Tom Brady</param-value>
  	</init-param>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HelloWorld</servlet-name>
  	<url-pattern>/HelloWorld</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SecondServlet</servlet-name>
  	<url-pattern>/SecondServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ThirdServlet</servlet-name>
  	<url-pattern>/ThirdServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletContextConfig</servlet-name>
  	<url-pattern>/ServletContextConfig</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  	<welcome-file>welcome1.html</welcome-file>
  </welcome-file-list>	
</web-app>
