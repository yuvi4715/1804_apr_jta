<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>HelloWorld</servlet-name>
  	<display-name>HelloWorld</display-name>
  	<description></description>
  	<servlet-class>com.revature.ers.servelets.HelloWorld</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Hello2</servlet-name>
  	<display-name>Hello2</display-name>
  	<description></description>
  	<servlet-class>com.revature.ers.servelets.Hello2</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FrontController</servlet-name>
  	<display-name>FrontController</display-name>
  	<description></description>
  	<servlet-class>com.revature.ers.servelets.FrontController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HelloWorld</servlet-name>
  	<url-pattern>/html/HelloWorld</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Hello2</servlet-name>
  	<url-pattern>/html/Hello2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FrontController</servlet-name>
  	<url-pattern>/html/FrontController</url-pattern>
  </servlet-mapping>
</web-app>
