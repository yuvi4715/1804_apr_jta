<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>FController</servlet-name>
		<display-name>FController</display-name>
		<description></description>
		<servlet-class>com.ranieri.servlet.FController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MasterServlet</servlet-name>
		<display-name>MasterServlet</display-name>
		<description></description>
		<servlet-class>com.ranieri.servlet.MasterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FController</servlet-name>
		<url-pattern>/FController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MasterServlet</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
</web-app>
