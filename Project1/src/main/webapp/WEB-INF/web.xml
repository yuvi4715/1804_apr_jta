<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>FrontController</servlet-name>
  	<display-name>FrontController</display-name>
  	<description></description>
  	<servlet-class>com.revature.requests.FrontController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserRequests</servlet-name>
  	<display-name>UserRequests</display-name>
  	<description></description>
  	<servlet-class>com.revature.requests.UserRequests</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ManagerRequests</servlet-name>
  	<display-name>ManagerRequests</display-name>
  	<description></description>
  	<servlet-class>com.revature.requests.ManagerRequests</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FrontController</servlet-name>
  	<url-pattern>/FrontController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserRequests</servlet-name>
  	<url-pattern>/UserRequests</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ManagerRequests</servlet-name>
  	<url-pattern>/ManagerRequests</url-pattern>
  </servlet-mapping>
</web-app>
